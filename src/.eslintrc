{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "env": {
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "noplusplus": [
            0
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "Extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/prop-types": [
            0
        ],
        "react/jsx-props-no-spreading": [
            0
        ],
        "import/prefer-default-export": [
            0
        ],
        "react/no-array-index-key": [
            0
        ]
    }
}